{
  "name": "typescript-template",
  "version": "0.1.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rm -fr dist/*",
    "test:run": "tape -r ts-node/register/transpile-only -r source-map-support/register -r ./test/bootstrap 'test/**/*.test.ts'",
    "test": "npm run test:run | tap-diff",
    "test:coverage": "nyc npm run test:run",
    "build": "tsc",
    "start": "node -r dotenv/config .",
    "start:watch": "ts-node-dev --transpileOnly -r dotenv/config src/index.ts",
    "lint": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'"
  },
  "keywords": [],
  "author": "Aptfire, LLC. <info@aptfire.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "github:aptfire/typescript-template.git"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^13.13.12",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "tap-diff": "^0.1.1",
    "tape": "^5.0.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.48",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "winston": "^3.2.1"
  }
}
